<% @survey.questions.each do |question| %>
  Question: <%= question.question %><br>
    <% question.answers.each do |answer| %>
    Answer: <%= answer.answer %><br>
      <% id = answer.id %>
      <% responses = Response.where("answer_id = ?", id) %>
      Count: <%= responses.length %><br>
    <% end %>

<% end %>
