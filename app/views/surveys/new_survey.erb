<div class= "container">
  <div class="page-header">
  <h1>Make a new Survey</h1>
  </div> <!-- page-header div   -->
  <div class="creation-container">
    <form action = "/users/<%=session[:user_id]%>/surveys" method="POST">
      <div class="input-field">
      Title: <input type = "text" name = "title" ></div><!-- closing input-field  div   -->
      <div class="input-field">
      Description: <input type = "text" name = "description"></div><!-- closing input-field  div   -->
      <input type = "submit">
    </form>
  </div> <!-- closing creation-container  -->
</div> <!-- closing container  -->
